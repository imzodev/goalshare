# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Postgres connection string for Drizzle migrations (server-only)
# Format: postgres://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=require
SUPABASE_DB_URL=

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Upstash Redis
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Rate limiting (defaults used if unset)
# Window in seconds (default: 60)
RATE_LIMIT_WINDOW_SECONDS=
# Per-window limit for authenticated users (default: 60)
RATE_LIMIT_LIMIT_AUTHED=
# Per-window limit for anonymous IPs (default: 60)
RATE_LIMIT_LIMIT_ANON=

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# AI provider API keys (server-only)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# OPENROUTER_API_KEY=

# Feature flags
# AI_ENABLE_MCP=

# AI (optional) – global defaults
# AI_DEFAULT_PROVIDER=openai
# AI_DEFAULT_MODEL=gpt-4o-mini

# AI (optional) – per-agent overrides
# Planner
# AI_PROVIDER_PLANNER=
# AI_MODEL_PLANNER=

# Smart
# AI_PROVIDER_SMART=
# AI_MODEL_SMART=

# Coach (example: OpenRouter DeepSeek)
# AI_PROVIDER_COACH=openrouter
# AI_MODEL_COACH=deepseek/deepseek-chat

# Scheduler
# AI_PROVIDER_SCHEDULER=
# AI_MODEL_SCHEDULER=

# Moderator
# AI_PROVIDER_MODERATOR=
# AI_MODEL_MODERATOR=

# Autocomplete
